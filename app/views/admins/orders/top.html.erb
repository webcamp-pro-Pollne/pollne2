<div class="container">
  <div class = "row">
    <div class = "col-md-1"> </div>
  <h2>注文履歴一覧</h2>
  <table>
    <thead>
      <tr>
        <th>購入日時</th>
        <th>購入者</th>
        <th>注文個数</th>
        <th>注文ステータス</th>
      </tr>
    </thead>
    <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.created_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  </div>
</div>