<h1>マイページ</h1>
<h3>会員情報</h3>

<table>
    <thead>
      <tr>
        <th>姓:<%= @customer.first_name %></th>
        <th>名:<%= @customer.last_name %></th>
      </tr>
      <tr>
        <th>姓(カナ):<%= @customer.first_name_kana %></th>
        <th>名(カナ):<%= @customer.last_name_kana %></th>
      </tr>
      <tr>
        <th>郵便番号:<%= @customer.postal_code %></th>
      </tr>
      <tr>
        <th>住所:<%= @customer.address %></th>
      </tr>
      <tr>
        <th>電話番号:<%= @customer.telephone_number %></th>
      </tr>
      <tr>
        <th>メールアドレス:<%= @customer.email %></th>
      </tr>
      </tr>
    </thead>
</table>


<h3>配送先一覧</h3>
<%= link_to "配送先一覧へ", addresses_path %>

<h3>会員情報編集へ</h3>
<%= link_to "会員情報編集へ", edit_customers_path %>

